{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "jquery": true
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "object-curly-newline": 0,
    "consistent-return": 0,
    "no-param-reassign": 0,
    "no-console": 0,
    "func-names": 0,
    "semi": ["warn", "always"],
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  }
}
